@model List<_1460353.Models.thongbao>
@foreach (var tt in Model)
{
    if (tt.daxem == 0)
    {
        <li style="border-color:black;background-color:#e6e6ff;">
            <a href="#">
                <div>@tt.ngaytao <h6 style="float:right;">Mới</h6></div>
                <h6 class="txtover">
                    @tt.noidung
                </h6>
            </a>
        </li>
    }
}
@foreach (var tt in Model)
{
    if (tt.daxem == 1)
    {
        <li style="border-color:black;background-color:white;">
            <a href="#">
                <div>@tt.ngaytao <h6 style="float:right;">Đã xem</h6></div>
                <h6 class="txtover">
                    @tt.noidung
                </h6>
            </a>
        </li>
    }
}
